#ifndef XlogViewer_Prefix_pch
#define XlogViewer_Prefix_pch

//获取沙盒 Document
#define kPathDocument [NSSearchPathForDirectoriesInDomains(NSDocumentDirectory, NSUserDomainMask, YES) firstObject]

//获取日志路径
#define kPathLog  [kPathDocument stringByAppendingFormat:@"/log"]


#endif /* XlogViewer_Prefix_pch */
